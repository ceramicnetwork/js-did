{
  "name": "dids",
  "version": "3.2.0",
  "description": "Typescript library for interacting with DIDs",
  "author": "Joel Thorstensson <oed@3box.io>",
  "license": "(Apache-2.0 OR MIT)",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14.14"
  },
  "sideEffects": false,
  "scripts": {
    "build:clean": "del dist",
    "build:js": "swc src -d ./dist --config-file ../../.swcrc",
    "build:types": "tsc --emitDeclarationOnly --skipLibCheck",
    "docs": "./node_modules/.bin/typedoc ./src/index.ts --excludePrivate --name DID --includeVersion ; touch docs/.nojekyll",
    "build": "pnpm run build:clean && pnpm run build:types && pnpm run build:js",
    "lint": "eslint src test --fix",
    "test": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js",
    "test:ci": "pnpm run test --ci --coverage",
    "prepare": "pnpm run build",
    "prepublishOnly": "package-check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ceramicnetwork/js-did.git"
  },
  "keywords": [
    "DID",
    "identity",
    "did-provider",
    "self-sovereign"
  ],
  "bugs": {
    "url": "https://github.com/ceramicnetwork/js-did/issues"
  },
  "homepage": "https://github.com/ceramicnetwork/js-did#readme",
  "dependencies": {
    "@stablelib/random": "^1.0.1",
    "ceramic-cacao": "^1.1.0",
    "dag-jose-utils": "^2.0.0",
    "did-jwt": "^6.0.0",
    "did-resolver": "^3.1.5",
    "multiformats": "^9.4.10",
    "rpc-utils": "^0.6.1",
    "uint8arrays": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@jest/globals": "^28.1.3",
    "@stablelib/x25519": "^1.0.2",
    "@types/jest": "^28.1.6",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "babel-jest": "^28.1.3",
    "eslint": "^8.6.0",
    "eslint-config-3box": "^0.4.0",
    "ethers": "^5.5.2",
    "jest": "^28.1.3",
    "prettier": "^2.5.1",
    "typedoc": "^0.23.10",
    "typescript": "^4.5.4",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  }
}
