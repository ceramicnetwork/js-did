{
  "name": "dids",
  "version": "3.2.0",
  "description": "Typescript library for interacting with DIDs",
  "author": "Joel Thorstensson <oed@3box.io>",
  "license": "(Apache-2.0 OR MIT)",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14.14"
  },
  "sideEffects": false,
  "scripts": {
    "build:clean": "del dist",
    "build:js": "swc src -d ./dist --config-file ../../.swcrc",
    "build:types": "tsc --emitDeclarationOnly --skipLibCheck",
    "build": "yarn build:clean && yarn build:types && yarn build:js",
    "docs": "./node_modules/.bin/typedoc ./src/index.ts --excludePrivate --name DID --includeVersion ; touch docs/.nojekyll",
    "lint": "eslint src --fix",
    "test": "yarn node --experimental-vm-modules $(yarn bin jest)",
    "test:ci": "yarn test --ci --coverage",
    "prepare": "yarn build",
    "prepublishOnly": "package-check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ceramicnetwork/js-did.git"
  },
  "keywords": [
    "DID",
    "identity",
    "did-provider",
    "self-sovereign"
  ],
  "bugs": {
    "url": "https://github.com/ceramicnetwork/js-did/issues"
  },
  "homepage": "https://github.com/ceramicnetwork/js-did#readme",
  "dependencies": {
    "@stablelib/random": "^1.0.1",
    "ceramic-cacao": "^1.1.0",
    "dag-jose-utils": "^2.0.0",
    "did-jwt": "^6.0.0",
    "did-resolver": "^3.1.5",
    "multiformats": "^9.4.10",
    "rpc-utils": "^0.6.1",
    "uint8arrays": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "babel-jest": "^27.4.6",
    "eslint": "^8.6.0",
    "eslint-config-3box": "^0.4.0",
    "ethers": "^5.5.2",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.4",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  }
}
