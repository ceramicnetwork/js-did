{
  "name": "@didtools/cacao",
  "version": "2.0.0",
  "description": "Typescript library for Ceramic OCAP",
  "author": "Haardik <hhaardik@uwaterloo.ca>",
  "license": "(Apache-2.0 OR MIT)",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14.14"
  },
  "sideEffects": false,
  "scripts": {
    "build:clean": "del dist",
    "build:js": "swc src -d ./dist --config-file ../../.swcrc",
    "build:types": "tsc --emitDeclarationOnly --skipLibCheck",
    "build": "pnpm run build:clean && pnpm run build:types && pnpm run build:js",
    "lint": "eslint src test --fix",
    "test": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js --passWithNoTests",
    "test:ci": "pnpm run test --ci --coverage",
    "prepare": "pnpm run build",
    "prepublishOnly": "package-check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ceramicnetwork/js-did.git"
  },
  "keywords": [
    "DID",
    "identity",
    "OCAP",
    "capabilities"
  ],
  "bugs": {
    "url": "https://github.com/ceramicnetwork/js-did/issues"
  },
  "homepage": "https://github.com/ceramicnetwork/js-did#readme",
  "dependencies": {
    "@ipld/dag-cbor": "^9.0.0",
    "apg-js": "^4.1.3",
    "caip": "^1.1.0",
    "multiformats": "^11.0.2",
    "uint8arrays": "^4.0.3"
  },
  "devDependencies": {
    "@types/luxon": "^3.3.0",
    "@types/node": "^20.1.2",
    "luxon": "^3.3.0"
  }
}
